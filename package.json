{
  "name": "@epic/centre-analytics",
  "version": "1.0.0",
  "description": "EPIC Centre Analytics component for cross-application login analytics recording",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
